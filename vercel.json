{
	"version": 2,
	"routes": [
		{
			"src": "/api/auth/(createuser|login|getuser)",
			"dest": "backend/routes/auth.js"
		},
		{
			"src": "/api/auth/(.*)",
			"dest": "backend/routes/auth.js"
		},
		{
			"src": "/api/(.*)",
			"dest": "backend/routes/$1"
		},
		{
			"src": "/(.*)",
			"dest": "backend/routes/$1"
		}
	]
}